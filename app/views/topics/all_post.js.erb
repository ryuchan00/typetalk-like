var post_data = '<% @post_data.each do |post| %>' +
    '<% user = post['name'] %>' +
    '<li class="media">' +
    '<div class="media-left">' +
    '<img class="media-object img-rounded" src="<%= post['imageUrl'] %>" alt="" height="50" width="50">' +
    '</div>' +
    '<div class="media-body">' +
    '<div>' +
    '<p><%= link_to 'typetalkで見る', "https://typetalk.in/topics/#{post['topic_id']}/posts/#{post['post_id']}", :target => ["_blank"] %></p>' +
    '<%= user %>' +
    '<span class="text-muted">posted at <%= post['created_at'] %></span>' +
    '</div>' +
    '<div>' +
    '<%= simple_format(post['message']) %>' +
    '</div>' +
    '<div>' +
    '<p class="col-xs-2 bg-danger">イイネの数 <%= post['like'] %></p>' +
    '</div>' +
    '</div>' +
    '</li>' +
    '<% end %>';

$("#ajax_list").html(post_data);
$("#time").html("<%= @time %>");
